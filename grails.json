{
  "variaveis_views": [
    "app/views/layouts/devise.haml",
    "app/views/layouts/application.haml",
    "app/views/shared/_navigation.haml"
  ],
  "variaveis_nome_projeto": [
    "package.json",
    "config/cable.yml"
  ],
  "variaveis_nome_database": [
    "config/database.yml",
    "config/environments/production.rb"
  ],
  "variaveis_constantize": [
    "config/application.rb"
  ],
  "adapters": {
    "postgresql": {
      "name": "pg",
      "version": "~> 1.2.3",
      "extra_gems": []
    },
    "sqlserver": {
      "name": "activerecord-sqlserver-adapter",
      "version": "~> 5.1",
      "extra_gems": [
        "'tiny_tds', '~> 2.1.2'"
      ]
    },
    "mysql": {
      "name": "mysql2",
      "version": null,
      "extra_gems": []
    },
    "sqlite": {
      "name": "sqlite3",
      "version": "~> 1.4",
      "extra_gems": []
    }
  }
}
